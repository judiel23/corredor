<%= simple_form_for(@version) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :make %>
    <%= f.input :model %>
    <%= f.input :code %>
    <%= f.input :version %>
    <%= f.input :descmotor %>
    <%= f.input :desctransmision %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
