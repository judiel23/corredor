<p id="notice"><%= notice %></p>

<h1>Listing Versions</h1>

<table>
  <thead>
    <tr>
      <th>Make</th>
      <th>Model</th>
      <th>Code</th>
      <th>Version</th>
      <th>Descmotor</th>
      <th>Desctransmision</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @versions.each do |version| %>
      <tr>
        <td><%= version.make %></td>
        <td><%= version.model %></td>
        <td><%= version.code %></td>
        <td><%= version.version %></td>
        <td><%= version.descmotor %></td>
        <td><%= version.desctransmision %></td>
        <td><%= link_to 'Show', version %></td>
        <td><%= link_to 'Edit', edit_version_path(version) %></td>
        <td><%= link_to 'Destroy', version, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= form_tag import_versions_path, multipart: true do %>
    <%= file_field_tag :file %>
    <%= submit_tag "Upload Versions" %>
<% end %>

<%= link_to 'New Version', new_version_path %>
